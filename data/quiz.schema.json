{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://example.com/root.json",
	"type": "array",
	"title": "EvalSMSI Quiz Schema",
	"items": {
		"$id": "#/items",
		"type": "object",
		"title": "Domain Schema",
		"required": [
			"numero",
			"libelle",
			"abrege",
			"subdomains"
		],
		"properties": {
			"numero": {
				"$id": "#/items/properties/numero",
				"type": "integer",
				"title": "Domain Number"
			},
			"libelle": {
				"$id": "#/items/properties/libelle",
				"type": "string",
				"title": "Domain Wording",
				"default": "",
				"pattern": "^(.*)$"
			},
			"abrege": {
				"$id": "#/items/properties/abrege",
				"type": "string",
				"title": "Domain Abstract",
				"default": "",
				"pattern": "^(.*)$"
			},
			"subdomains": {
				"$id": "#/items/properties/subdomains",
				"type": "array",
				"title": "Subdomain Schema",
				"items": {
					"$id": "#/items/properties/subdomains/items",
					"type": "object",
					"title": "Subdomain Schema",
					"required": [
						"numero",
						"libelle",
						"comment",
						"questions"
					],
					"properties": {
						"numero": {
							"$id": "#/items/properties/subdomains/items/properties/numero",
							"type": "integer",
							"title": "SubDomain Number"
						},
						"libelle": {
							"$id": "#/items/properties/subdomains/items/properties/libelle",
							"type": "string",
							"title": "SubDomain Wording",
							"default": "",
							"pattern": "^(.*)$"
						},
						"comment": {
							"$id": "#/items/properties/subdomains/items/properties/comment",
							"type": "string",
							"title": "SubDomain Comment",
							"default": "",
							"pattern": "^(.*)$"
						},
						"questions": {
							"$id": "#/items/properties/subdomains/items/properties/questions",
							"type": "array",
							"title": "Questions Schema",
							"items": {
								"$id": "#/items/properties/subdomains/items/properties/questions/items",
								"type": "object",
								"title": "Questions Schema",
								"required": [
									"numero",
									"libelle",
									"mesure",
									"poids",
									"level"
								],
								"properties": {
									"numero": {
										"$id": "#/items/properties/subdomains/items/properties/questions/items/properties/numero",
										"type": "integer",
										"title": "Question Number"
									},
									"libelle": {
										"$id": "#/items/properties/subdomains/items/properties/questions/items/properties/libelle",
										"type": "string",
										"title": "Question Wording",
										"default": "",
										"pattern": "^(.*)$"
									},
									"mesure": {
										"$id": "#/items/properties/subdomains/items/properties/questions/items/properties/mesure",
										"type": "string",
										"title": "Question Recommandation",
										"default": "",
										"pattern": "^(.*)$"
									},
									"poids": {
										"$id": "#/items/properties/subdomains/items/properties/questions/items/properties/poids",
										"type": "integer",
										"title": "Question Weight"
									},
									"level": {
										"$id": "#/items/properties/subdomains/items/properties/questions/items/properties/level",
										"type": "integer",
										"title": "Question Level"
									}
								}
							}
						}
					}
				}
			}
		}
	}
}
